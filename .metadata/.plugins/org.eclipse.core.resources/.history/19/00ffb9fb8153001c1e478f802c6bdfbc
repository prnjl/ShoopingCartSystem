package com.casestudy.service;

import java.util.List;

import com.casestudy.model.Product;

public interface ProductService {

	 
	   
	List<Product> getAllProducts();
	Product getProductByName(String productName);
	Product getProductByCategory(String productCategory);
	Product getProductByType(String productType);
	Product addProduct(Product product);
	String updateProduct(Product product , String productName);
	String deleteByProductName(Product product , String productName);
	
}
